# Forth AI Underwriting - Docker Production Configuration
# Copy this file to docker-production.env and fill in your actual values

# === Webhook Service Configuration ===
WEBHOOK_QUEUE_NAME=uw-contracts-parser-dev-sqs
WEBHOOK_AWS_REGION=us-west-1
WEBHOOK_FORTH_API_BASE_URL=https://api.forthcrm.com/v1
WEBHOOK_FORTH_API_KEY=your_forth_api_key_here

# === SQS Queue Configuration ===
SQS_MAIN_QUEUE=uw-contracts-parser-dev-sqs
SQS_DLQ=uw-contracts-parser-dl-dev-sqs

# === AWS Configuration ===
AWS_REGION=us-west-1
USE_LOCAL_QUEUE=false

# AWS Credentials (replace with your actual credentials)
AWS_ACCESS_KEY_ID=AKIA_YOUR_ACCESS_KEY_HERE
AWS_SECRET_ACCESS_KEY=your_secret_access_key_here

# === Environment ===
ENVIRONMENT=development
LOG_LEVEL=INFO

# === S3 Configuration ===
AWS_S3_BUCKET_NAME=contact-contracts-dev-s3-us-west-1
S3_BUCKET_NAME=contact-contracts-dev-s3-us-west-1
S3_REGION=us-west-1

# === Application Settings ===
APP_VERSION=1.0.0

# === Database Configuration (Required for settings loading) ===
# Minimal database config to prevent errors (not used by microservices)
DB_NAME=forth_underwriting_dev
DB_USER=postgres
DB_PASSWORD=postgres
DB_HOST=localhost
DB_PORT=5432

# === Forth API Configuration ===
FORTH_API_BASE_URL=https://api.forthcrm.com/v1
FORTH_API_KEY=your_forth_api_key_here
FORTH_API_KEY_ID=your_api_key_id

# === Queue Configuration ===
QUEUE_NAME=uw-contracts-parser-dev-sqs

# === Feature Flags ===
ENABLE_DOCUMENT_ENHANCEMENT=true
ENABLE_METRICS=true

# === Service Configuration ===
MAX_CONCURRENT_DOWNLOADS=3
POLL_INTERVAL_SECONDS=5

# === Security Configuration ===
SECRET_KEY=change_this_to_a_secure_random_key_in_production_minimum_32_characters
